{
  "name": "pccomponentes/documentation-bundle",
  "description": "PcComponentes Documentation Bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "JosÃ© Gilberto Mullor",
      "email": "gilberto.mullor@pccomponentes.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PcComponentes\\DocumentationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PcComponentes\\DocumentationBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4 | ^8",
    "ext-json": "*",
    "symfony/http-foundation": ">=4.4 <=6",
    "symfony/dependency-injection": ">=4.4 <=6",
    "symfony/config": ">=4.4 <=6",
    "symfony/http-kernel": ">=4.4 <=6",
    "symfony/routing": ">=4.4 <=6",
    "symfony/yaml": ">=4.4 <=6",
    "pccomponentes/ddd": "^2.0"
  },
  "require-dev": {
    "pccomponentes/coding-standard": "^1.0",
    "phpunit/phpunit": "^9",
    "squizlabs/php_codesniffer": "3.*"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
